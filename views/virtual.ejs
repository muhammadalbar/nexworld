<script>
  function navigateFrame(media) {
    $("#virtual3dFrame").attr("src", `3d/index.htm?media=${media}`);
  }
  $("#expo-list").hide();
  function autoPlayYouTubeModal() {
    var triggerOpen = $("body").find("[data-tagVideo]");
    triggerOpen.click(function () {
      var theModal = $(this).data("bs-target"),
        videoSRC = $(this).attr("data-tagVideo"),
        videoSRCauto = videoSRC + "?autoplay=1";
      $(theModal + " iframe").attr("src", videoSRCauto);
      $(theModal + " button.btn-close").click(function () {
        $(theModal + " iframe").attr("src", videoSRC);
      });
    });
  }
  function openNewWindow() {
    window.open("/special-deal");
  }
  $("#submit-email").click(function (e) {
    // e.preventDefault();
    console.log("SEND EMAIL");
  });

  // $("#expo").click(function () {
  //   $("#expo-list").fadeIn(500);
  //   $("#left-card").hide();
  // });
  // $("#expo-close").click(function () {
  //   $("#expo-list").fadeOut(500);
  //   $("#left-card").fadeIn(500);
  // });

  $(document).ready(function () {
    autoPlayYouTubeModal();
    $("#left-card").hide();
    $("#right-card").hide();
    $("#demo").modal("show");

    if ($(window).width() <= 731) {
      $("#right-card").hide();
      $("#right-card-show").hide();
      $("#leftmenu").click(function () {
        $("#left-card").fadeOut(500);
        $(".welcome-text").fadeIn(500);
        $("#right-card-show").show();
      });
      $("#rightmenu").click(function () {
        $("#right-card").fadeOut(500);
        $("#left-card-show").show();
      });
      $("#left-card-show").click(function () {
        $("#left-card").fadeIn(500);
        $(".welcome-text").fadeOut(500);
        $("#right-card-show").hide();
        $("#left-card-show").hide();
      });
      $("#right-card-show").click(function () {
        $("#right-card").fadeIn(500);
        $("#left-card-show").hide();
        $("#right-card-show").hide();
      });
    } else {
      $("#leftmenu").click(function () {
        $("#left-card").fadeOut(500);
        $(".welcome-text").fadeIn(500);
        $("#left-card-show").show();
      });
      $("#rightmenu").click(function () {
        $("#right-card").fadeOut(500);
        $("#right-card-show").show();
      });
      $("#left-card-show").click(function () {
        $("#left-card-show").hide();
        $(".welcome-text").fadeOut(500);
        $("#left-card").fadeIn(500);
      });
      $("#right-card-show").click(function () {
        $("#right-card-show").hide();
        $("#right-card").fadeIn(500);
      });
    }
    $(".container-fluid").append(`
    <div class="position-absolute top-0 start-0 welcome-text p-2 mt-3 ms-2">
      Welcome, ${localStorage.getItem("synnex-username")} !
    </div>
    `);
  });
</script>

