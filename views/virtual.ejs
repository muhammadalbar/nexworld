<script>
  fetch(
    "https://api.nevvorld.com/api/token/app/findRegistRankList?eventToken=UY9WOEUV",
    {
      method: "GET",
    }
  )
    .then((response) => response.json())
    .then((data) => {
      data.data.map((e, i) => {
        $("#modal-leaderboard").append(`
                <div class="row">
                  <div class="col-4 text-center text-nowrap overflow-auto"><strong>${
                    i + 1
                  }.</strong></div>
                  <div class="col-4 text-center text-nowrap overflow-auto"><h5><strong>${
                    e.ign
                  }</strong></h5></div>
                  <div class="col-4 text-center text-nowrap overflow-auto"><strong>${
                    e.point
                  }</strong></div>
                </div>
              `);
      });
      data.data.slice(0, 5).map((e) => {
        $("#lead-list").append(`
              <li>
                <div>
                  <p class="overflow-auto text-nowrap me-2">${e.ign}</p>
                  <span>${e.point}</span>
                </div>
              </li>
              `);
      });
    })
    .catch((err) => console.log(err));
</script>
