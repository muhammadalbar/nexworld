    <script>
      if (
        localStorage.getItem("synnex-user") &&
        localStorage.getItem("synnex-jwt")
      ) {
        $("#login-nav").remove();
        $("#getyoupass").remove();
      } else {
        $("#virtual-nav").remove();
        $("#logout-nav").remove();
      }

      function logout() {
        localStorage.removeItem("synnex-user");
        localStorage.removeItem("synnex-jwt");

        window.location.href = "/";
      }
    </script>
